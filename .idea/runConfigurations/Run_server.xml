<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Run server" type="NodeJSConfigurationType" factoryName="Node.js" path-to-node="project" path-to-js-file="dist/index.js" node-parameters="" working-dir="$PROJECT_DIR$">
    <envs>
      <env name="VULCAIN_ENV" value="<%=env%>" />
      <env name="VULCAIN_SERVICE_VERSION" value="1.0" />
      <env name="VULCAIN_SERVICE_NAME" value="<%= project.fullName %>" />
    </envs>
    <method>
      <option name="GulpBeforeRunTask" enabled="true">
        <node-interpreter>project</node-interpreter>
        <node-options />
        <gulpfile>$PROJECT_DIR$/gulpFile.js</gulpfile>
        <tasks>
          <task>compile-ts</task>
        </tasks>
        <arguments />
        <envs />
      </option>
    </method>
  </configuration>
</component>
